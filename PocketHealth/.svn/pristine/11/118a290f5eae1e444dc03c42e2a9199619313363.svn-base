#import "PHCountManager.h"
static PHCountManager *phCountManager;
@implementation PHCountManager
+(id)defaultManager
{
    if (phCountManager == nil) {
        phCountManager = [[PHCountManager alloc] init];
    }
    return phCountManager;
}

